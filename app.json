{
  "expo": {
    "name": "MilesXP Daily",
    "slug": "milesxp-daily",
    "owner": "miittulrana55",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.milesxp.daily",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to track work routes and ensure driver safety.",
        "NSLocationWhenInUseUsageDescription": "This app needs location access to track work routes and ensure driver safety.",
        "UIBackgroundModes": ["location", "background-fetch", "background-processing"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.milesxp.daily",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs location access to track work routes and ensure driver safety.",
          "locationWhenInUsePermission": "This app needs location access to track work routes and ensure driver safety.",
          "locationAlwaysPermission": "This app needs location access to track work routes and ensure driver safety.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ],
    "scheme": "milesxp-daily",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3ea31db3-72cd-4096-a07a-cde10dc6466a"
      }
    }
  }
}